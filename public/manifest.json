{
  "manifest_version": 3,
  "name": "Extension Name",
  "version": "0.1",
  "description": "Some Description",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [{
    "js": [ "content.js" ],
    "matches": [
       "https://www.youtube.com/*"
    ]
  }],
  "icons": {
    "192": "logo192.png"
  },
  "action": {
    "default_icon": {
      "192": "logo192.png"
    },
    "default_title": "Popup",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html"
  }
}
