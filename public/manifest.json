{
  "manifest_version": 3,
  "name": "Extension Name",
  "version": "0.1",
  "description": "Some Description",
  "permissions": [
    "storage",
    "scripting",
    "activeTab",
    "webNavigation"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*"
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "content_scripts": [{
      "js": [ "content.js" ],
      "matches": ["*://www.youtube.com/watch?v=*"],
      "run_at": "document_start",
      "type": "module"
    }],
  "icons": {
    "192": "logo192.png"
  },
  "action": {
    "default_icon": {
      "192": "logo192.png"
    },
    "default_title": "Popup",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html"
  }
}